
<div class="jumbotron">
  <p id="alert"><%= alert %></p>
  <%= form_tag sessions_path do %>
    <h2>Login Here!</h2>
    <div class="field">
      <%= label_tag :email %>
      <br>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <br>
    <div class="field">
      <%= label_tag :password %>
      <br>
      <%= password_field_tag :password %>
    </div>
    <br>
    <div class="actions"><%= submit_tag "Log in" %>
    </div>

  <% end %>
</div>
